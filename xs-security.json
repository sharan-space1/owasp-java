{
  "xsappname": "owasp-java",
  "tenant-mode": "dedicated",
  "scopes": [
    {
      "name": "$XSAPPNAME.user",
      "description": "user"
    },
    {
      "name": "$XSAPPNAME.admin",
      "description": "admin"
    }
  ],
  "attributes": [],
  "role-templates": [
    {
      "name": "user",
      "description": "OWASP JAVA User",
      "scope-references": ["$XSAPPNAME.user"],
      "attribute-references": []
    },
    {
      "name": "admin",
      "description": "OWASP JAVA Admin",
      "scope-references": ["$XSAPPNAME.admin"],
      "attribute-references": []
    }
  ],
  "role-collections": [
    {
      "name": "OWASP JAVA User",
      "description": "OWASP JAVA User",
      "role-template-references": ["$XSAPPNAME.user"]
    },
    {
      "name": "OWASP JAVA Admin",
      "description": "OWASP JAVA Admin",
      "role-template-references": ["$XSAPPNAME.admin"]
    }
  ],
  "oauth2-configuration": {
    "token-validity": 3600,
    "redirect-uris": [
      "https://oauth.pstmn.io/v1/callback",
      "http://localhost:*/login/callback",
      "https://*.hana.ondemand.com/**",
      "https://*.applicationstudio.cloud.sap/**"
    ]
  }
}